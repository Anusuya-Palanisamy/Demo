{
    "name": "easybridge-search-web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "coverage": "ng test --code-coverage",
        "lint": "ng lint",
        "lint-locales": "jsonlint -q src/assets/i18n/en.json",
        "csslint": "stylelint '**/*.scss'; exit 0;",
        "csslint-fix": "stylelint '**/*.scss' --fix",
        "git-version": "ts-node git-version.ts",
        "build-env": "npm run build -- -c=$ENV",
        "build-stage": "ENV=stage npm run build-env",
        "build-nightly": "npm run pre-build && ng build --configuration=nightly",
        "build-prod": "npm run pre-build && ng build --configuration=production",
        "test-coverage": "ng test --code-coverage",
        "pre-build": "npm run lint && npm run test-coverage",
        "fetch-gql-schema": "npx apollo schema:download ./schema/graphql-schema.json",
        "generate-gql-types": "npx apollo codegen:generate --addTypename --target=typescript --localSchemaFile=./schema/graphql-schema.json --queries=./src/app/**/*.ts --globalTypesFile=src/types/graphql-global-types.ts types",
        "generate-schema-local": "npm run fetch-gql-schema -- --endpoint=http://localhost:3000/graphql && npm run generate-gql-types",
        "generate-schema-nightly": "npm run fetch-gql-schema -- --endpoint=https://easybridge-user-search-service.nightly.savvasrealizedev.com/graphql && npm run generate-gql-types"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~11.0.1",
        "@angular/cdk": "^11.0.0",
        "@angular/common": "~11.0.1",
        "@angular/compiler": "~11.0.1",
        "@angular/core": "~11.0.1",
        "@angular/elements": "^11.0.3",
        "@angular/forms": "~11.0.1",
        "@angular/material": "^11.0.0",
        "@angular/platform-browser": "~11.0.1",
        "@angular/platform-browser-dynamic": "~11.0.1",
        "@angular/router": "~11.0.1",
        "@apollo/client": "^3.0.0",
        "@lst/cel-components": "^2.1.9",
        "@lst/font-avenir": "^2.1.2",
        "@lst/font-cabin": "^1.1.2",
        "@lst/k12_universal_icons": "^1.54.0",
        "@lst/lst-auth": "^1.2.7",
        "@lst/post-messaging": "0.0.6",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "ngx-csv": "^0.3.2",
        "apollo-angular": "^2.1.0",
        "graphql": "^15.0.0",
        "lodash.throttle": "^4.1.1",
        "penpal": "^5.2.1",
        "rxjs": "~6.6.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.2",
        "@angular-eslint/builder": "1.2.0",
        "@angular-eslint/eslint-plugin": "1.2.0",
        "@angular-eslint/eslint-plugin-template": "1.2.0",
        "@angular-eslint/template-parser": "1.2.0",
        "@angular/cli": "~11.0.2",
        "@angular/compiler-cli": "~11.0.1",
        "@lst/stylelint-config-lst": "0.0.1",
        "@lst/eslint-config-angular-lst": "0.0.2",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.3.0",
        "@typescript-eslint/parser": "4.3.0",
        "eslint": "^7.6.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "typescript": "~4.0.5"
    }
}
